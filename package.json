{
  "name": "playground",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "postinstall": "npx typesync",
    "prestart:dev": "yarn run clean:cache",
    "start:dev": "NODE_OPTIONS='--inspect' nx serve compare-detail-pages-ux --verbose",
    "start:prod": "nx serve compare-detail-pages-ux --prod",
    "build": "nx build compare-detail-pages-ux --prod",
    "lint": "nx lint compare-detail-pages-ux",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:lint": "nx affected:lint",
    "affected:depGraph": "nx affected:dep-graph",
    "analyze": "ANALYZE=true yarn run build",
    "workspace:update": "nx migrate latest",
    "postworkspace:update": "zx libs/script/src/lib/postWorkspace:update.mjs",
    "dep-graph": "nx dep-graph",
    "check:unusedModules": "npx depcheck --ignores='tslib,dotenv,@nrwl/*,prettier,@types/*,@babel/*,@typescript-eslint/eslint-plugin,eslint-plugin*'",
    "check:outdatedModules": "yarn outdated",
    "generate:getAccessTokenWithHttp": "export $(cat .env | xargs) && ts-node libs/http/src/lib/getAccessTokenWithHttp.ts",
    "generate:typeHelper": "ts-node -P libs/script/tsconfig.json -r tsconfig-paths/register libs/script/src/lib/typeHelper.ts",
    "postgenerate:typeHelper": "prettier --write libs/ui-feature/dist/type/assetTypeHelper.ts",
    "clean:cache": "rm -rf dist",
    "clean": "yarn run clean:cache && rm -rf node_modules",
    "postclean": "yarn",
    "create:stories": "sh sol.sh"
  },
  "private": true,
  "dependencies": {
    "@next/bundle-analyzer": "^13.5.5",
    "@tanstack/query-core": "^5.0.0",
    "axios": "^1.0.0",
    "next": "13.4.1",
    "next-compose-plugins": "^2.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-is": "18.2.0",
    "styletron-engine-atomic": "^1.5.0",
    "styletron-react": "6.1.0",
    "tslib": "2.3.0"
  },
  "devDependencies": {
    "@nx/devkit": "16.10.0",
    "@nx/esbuild": "16.10.0",
    "@nx/eslint-plugin": "16.10.0",
    "@nx/jest": "16.10.0",
    "@nx/js": "16.10.0",
    "@nx/linter": "16.10.0",
    "@nx/next": "16.10.0",
    "@nx/node": "16.10.0",
    "@nx/playwright": "16.10.0",
    "@nx/react": "16.10.0",
    "@nx/vite": "16.10.0",
    "@nx/web": "16.10.0",
    "@nx/workspace": "16.10.0",
    "@playwright/test": "^1.36.0",
    "@swc/core": "~1.3.85",
    "@swc/plugin-styled-components": "^1.5.67",
    "@testing-library/react": "14.0.0",
    "@types/eslint": "~8.44.4",
    "@types/eslint-config-prettier": "6.11.1",
    "@types/http-server": "^0.12.2",
    "@types/jest": "29.4.4",
    "@types/jsdom": "~21.1.3",
    "@types/node": "18.14.2",
    "@types/prettier": "^2.7.3",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.0",
    "@types/react-is": "18.2.0",
    "@types/styletron-engine-atomic": "^1.1.0",
    "@types/styletron-react": "^5.0.3",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "@vitest/coverage-c8": "~0.32.0",
    "@vitest/ui": "~0.32.0",
    "babel-jest": "29.4.3",
    "babel-plugin-styled-components": "1.10.7",
    "esbuild": "^0.17.17",
    "eslint": "8.46.0",
    "eslint-config-next": "13.4.1",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-playwright": "^0.15.3",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "http-server": "^14.1.1",
    "jest": "29.4.3",
    "jest-environment-jsdom": "29.4.3",
    "jest-environment-node": "^29.4.1",
    "jsdom": "~22.1.0",
    "lit": "^2.8.0",
    "nx": "16.10.0",
    "prettier": "^2.6.2",
    "swc-loader": "0.1.15",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.1.6",
    "vite": "~4.3.9",
    "vitest": "~0.32.0"
  },
  "resolutions": {
    "@types/react": "18.2.0"
  }
}
